
public class repeat{
    public static void main(String args[]){
        String name="my name is str str str str";
        String[] a=name.split(" ");
        String find="str";
        int count =0;
        
        for(int i=0;i<a.length;i++)
        {
            if (a[i].equals(find)){
                count=count+1;
            }
        }
        System.out.println(count);
    }
}
