class main {
    public static void main (String args[]){
        String a="I am a automation tester";
        String b=" ";
        for (int i=0;i<a.length();i++)
        {
            if (a.charAt(i) !=' ')
            {
                b=b+a.charAt(i);
            }
        }
        System.out.println(b);
    }
}
